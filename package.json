{
	"name": "4399-on-vscode",
	"displayName": "4399 on vscode",
	"description": "在vscode上玩4399小游戏, 帮助你劳逸结合, 提高开发效率",
	"version": "0.1.2",
	"engines": {
		"vscode": "^1.67.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "4399 on vscode",
			"properties": {
				"4399-on-vscode.user-agent": {
					"description": "4399 on vscode 发出的请求头中的 UA",
					"type": "string",
					"default": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/103.0.0.0 Safari/537.36"
				},
				"4399-on-vscode.cookie": {
					"description": "4399 on vscode 发出的请求头中的 cookie, 该设置可能会由此扩展自动更改, 您设置的 cookie 无法用于登陆账号",
					"type": "string",
					"default": ""
				},
				"4399-on-vscode.referer": {
					"description": "4399 on vscode 发出的请求头中的 referer",
					"type": "string",
					"default": "https://www.4399.com/"
				},
				"4399-on-vscode.outputLogs": {
					"description": "控制是否输出日志",
					"type": "boolean",
					"default": false
				},
				"4399-on-vscode.moreOpen": {
					"description": "控制是否允许游戏多开",
					"type": "boolean",
					"default": false
				},
				"4399-on-vscode.title": {
					"description": "控制 Webview 标题, 若留空则使用游戏名称或 4399 on vscode",
					"type": "string",
					"default": ""
				}
			}
		},
		"commands": [
			{
				"command": "4399-on-vscode.old-flash-games",
				"title": "4399 on vscode: 旧的 flash 游戏",
				"shortTitle": "4399 on vscode: special"
			},
			{
				"command": "4399-on-vscode.special",
				"title": "4399 on vscode: 推荐",
				"shortTitle": "4399 on vscode: search"
			},
			{
				"command": "4399-on-vscode.get",
				"title": "4399 on vscode: 输入游戏 id",
				"shortTitle": "4399 on vscode: get"
			},
			{
				"command": "4399-on-vscode.search",
				"title": "4399 on vscode: 搜索",
				"shortTitle": "4399 on vscode: search"
			}
		]
	},
	"scripts": {
	  "vscode:prepublish": "npm run compile",
	  "compile": "tsc -p ./",
	  "watch": "tsc -watch -p ./",
	  "pretest": "npm run compile && npm run lint",
	  "lint": "eslint src --ext ts",
	  "test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
	  "@types/vscode": "^1.70.0",
	  "@types/glob": "^7.2.0",
	  "@types/mocha": "^9.1.1",
	  "@types/node": "16.x",
	  "@typescript-eslint/eslint-plugin": "^5.31.0",
	  "@typescript-eslint/parser": "^5.31.0",
	  "eslint": "^8.20.0",
	  "glob": "^8.0.3",
	  "mocha": "^10.0.0",
	  "typescript": "^4.7.4",
	  "@vscode/test-electron": "^2.1.5"
	},
	"keywords": [
		"小霸王",
		"游戏",
		"flash",
		"games",
		"高效工作"
	],
	"publisher": "dsy4567",
	"license": "SEE LICENSE IN LICENSE",
	"dependencies": {
		"axios": "^0.27.2",
		"cheerio": "^1.0.0-rc.12",
		"iconv-lite": "^0.6.3"
	},
	"icon": "icon.png",
	"repository": "https://github.com/dsy4567/4399-on-vscode.git",
	"author": "dsy4567",
	"bugs": {
		"email": "dsy4567@outlook.com",
		"url": "https://github.com/dsy4567/4399-on-vscode/issues"
	},
	"homepage": "https://github.com/dsy4567/4399-on-vscode#readme"
}